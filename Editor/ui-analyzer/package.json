{
  "name": "ui-analyzer",
  "private": true,
  "version": "0.1.0",
  "author": {
    "name": "Jiyan Hu"
  },
  "main": "dist/main/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:electron": "tsc -p tsconfig.main.json",
    "start": "electron ./dist/main/main.js",
    "watch:electron": "nodemon --watch src/main --exec \"npm run build:electron && npm run start\"",
    "dist": "npm run build && npm run build:electron && electron-builder"
  },
  "dependencies": {
    "moment": "^2.30.1",
    "winston": "^3.12.0",
    "@mdi/font": "^7.4.47",
    "jsonc-parser": "^3.3.1",
    "lodash": "^4.17.21",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.8.1",
    "vue": "^3.5.13",
    "vuetify": "^3.7.5"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.2",
    "vue-tsc": "^2.2.0",
    "get-port": "^7.0.0",
    "vite": "^6.0.6"
  },
  "build": {
    "appId": "com.liberrpa.ui-analyzer",
    "productName": "UI Analyzer",
    "directories": {
      "output": "G:/Temp/Electron"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extraFiles": [
      {
        "from": "LICENSE",
        "to": "LICENSE"
      }
    ],
    "icon": "assets/LiberRPA_icon_v3_color_UiAnalyzer_256.ico",
    "win": {
      "target": "portable",
      "defaultArch": "x64"
    }
  }
}
